<?xml version='1.0' encoding='utf-8'?>
<form>

	<fieldset>
		<input type="time" />
	</fieldset>

	<label>Overview</label>
	<row>
		<chart>
			<searchName>messages_by_severity_over_time</searchName>
			<title>Messages by Severity (over time)</title>
			<option name="charting.chart">area</option>
			<option name="count">10</option>
			<option name="displayRowNumbers">true</option>
		</chart>
		<table>
			<searchName>messages_by_severity</searchName>
			<title>Messages by Severity</title>
			<option name="count">10</option>
			<option name="displayRowNumbers">true</option>
			<drilldown>
				<link><![CDATA[/app/django/flashtimeline/?q=search%20sourcetype%3Ddjango%20severity%3D"$row.severity$"&earliest=$earliest$&latest=$latest$]]></link>
			</drilldown>
		</table>
	</row>
	<row>
		<table>
			<searchName>messages_by_module</searchName>
			<title>Messages by Logger Name</title>
			<option name="count">10</option>
			<option name="displayRowNumbers">true</option>
			<drilldown>
				<link><![CDATA[/app/django/flashtimeline/?q=search%20sourcetype%3Ddjango%20module%3D"$row.module$"&earliest=$earliest$&latest=$latest$]]></link>
			</drilldown>
		</table>
		<table>
			<searchString>sourcetype="django" NOT (tag=database_query OR tag=database_change) | eval severity=lower(vendor_severity) | fillnull value="(undefined)" message | table message severity | head 1000</searchString>
			<title>Most Recent Messages (Excluding SQL Logs)</title>
			<option name="count">10</option>
			<option name="displayRowNumbers">true</option>
		</table>
	</row>

</form>
